node{
   stage('SCM Checkout'){
     git 'https://github.com/javahometech/my-app'
   }
stage('Compile-Package'){
     sh 'mvn clean'
     sh 'mvn package'
     }
stage('deploying war to web server')
sshagent(['webserver']) {
     sh 'scp -r StrictHostKeyChecking=no target/*.war  ec2-user@34.207.192.47:/opt/apache-tomcat-9.0.21/webapps/'
    // copy war to webserver
     }
    }
