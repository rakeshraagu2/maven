node{
   stage('SCM Checkout'){
     git 'https://github.com/javahometech/my-app'
   }
stage('Compile-Package'){
     sh 'mvn clean'
     sh 'mvn package'
sshagent(credentials:['webserver']) {
     sh 'scp -v -o StrictHostKeyChecking=no target/*.war ec2-user@52.207.209.251:/opt/apache-tomcat-9.0.21/webapps/'
    // copy war to webserver
 
}
     }
    }
